{
  "name": "@feltcoop/eslint-config",
  "version": "0.1.6",
  "license": "Unlicense",
  "description": "an ESLint config for TypeScript, Svelte, and Prettier",
  "homepage": "https://github.com/feltcoop/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/feltcoop/eslint-config.git"
  },
  "author": {
    "name": "Felt.coop",
    "email": "team@felt.social",
    "url": "https://felt.social"
  },
  "main": "index.cjs",
  "type": "module",
  "exports": {
    ".": "./index.cjs"
  },
  "files": [
    "changelog.md",
    "index.cjs"
  ],
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "engines": {
    "node": ">= 16.6"
  },
  "scripts": {
    "start": "gro dev",
    "dev": "gro dev",
    "build": "gro build",
    "test": "gro test",
    "deploy": "gro deploy"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "eslint": ">=8",
    "eslint-plugin-svelte3": ">=3",
    "typescript": ">=4"
  },
  "prettier": {
    "useTabs": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
